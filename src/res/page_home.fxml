<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" prefHeight="400.0" prefWidth="600.0" vgap="10"
          xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.HomeController">
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>
    <ScrollPane prefHeight="400.0" prefWidth="600.0" hbarPolicy="NEVER" vbarPolicy="AS_NEEDED">
        <HBox prefHeight="390.0" prefWidth="600.0">
            <VBox fx:id="projectNameVbox" alignment="TOP_LEFT" prefHeight="400.0" prefWidth="575.0"
                  HBox.hgrow="SOMETIMES"/>
            <VBox fx:id="projectManagerVbox" alignment="TOP_CENTER" prefHeight="400.0" prefWidth="575.0"
                  HBox.hgrow="SOMETIMES"/>
            <VBox fx:id="viewButtonsVbox" alignment="TOP_RIGHT" prefHeight="400.0" prefWidth="575.0"
                  HBox.hgrow="SOMETIMES"/>
        </HBox>
    </ScrollPane>
</GridPane>
